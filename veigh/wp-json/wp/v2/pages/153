3o\r\n\r\n                \/\/ Obter a URL atual e os par\u00e2metros UTM\r\n                const currentUrl = new URL(window.location.href);\r\n                const searchParams = currentUrl.searchParams;\r\n                const utmParams = new URLSearchParams();\r\n\r\n                \/\/ Copiar par\u00e2metros UTM\r\n                Array.from(searchParams.entries()).forEach(param => {\r\n                    if (param[0].startsWith('utm_')) {\r\n                        utmParams.append(param[0], param[1]);\r\n                    }\r\n                });\r\n\r\n                \/\/ Construir a nova URL com par\u00e2metros UTM e redirecionar\r\n                window.location.href = `${destinationUrl}&${utmParams}`;\r\n            });\r\n        });\r\n    }\r\n\r\n    \/\/ Configurar os bot\u00f5es com os textos e URLs de destino correspondentes\r\n    configureButton(\"Quero investigar meu c\u00f4njuge (marido\/namorado)\", \"https:\/\/tecnocia.up.railway.app\/whatspy-2\/?src=hm\");\r\n    configureButton(\"Quero investigar meu c\u00f4njuge (esposa\/namorada)\", \"https:\/\/tecnocia.up.railway.app\/whatspy-2\/?src=ml\");\r\n});\r\n<\/script>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-9444854 elementor-widget elementor-widget-heading\" data-id=\"9444854\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<style>\/*! elementor - v3.20.0 - 26-03-2024 *\/\n.elementor-heading-title{padding:0;margin:0;line-height:1}.elementor-widget-heading .elementor-heading-title[class*=elementor-size-]>a{color:inherit;font-size:inherit;line-height:inherit}.elementor-widget-heading .elementor-heading-title.elementor-size-small{font-size:15px}.elementor-widget-heading .elementor-heading-title.elementor-size-medium{font-size:19px}.elementor-widget-heading .elementor-heading-title.elementor-size-large{font-size:29px}.elementor-widget-heading .elementor-heading-t